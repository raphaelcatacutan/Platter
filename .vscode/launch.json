{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Module",
			"type": "debugpy",
			"request": "launch",
			"module": "app.main",
			"args": [
				"${file}"
			],
			"env": {
				"PYTHONPATH": "."
			},
			"cwd": "${workspaceFolder}/platter-compiler-sveltejs/static/python"
		}
	]
}
